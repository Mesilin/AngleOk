@model Data.AngleOk.Model.Models.RealtyObject

@{
	ViewData["Title"] = "Удаление объекта";
}

<h2>Удаление объекта</h2>

<h3>Вы действительно хотите удалить этот объект недвижимости?</h3>
<div>
	<h4>Объект</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">
			Кадастровый номер
		</dt>
		<dd class="col-sm-10">
			@Model.CadastralNumber
		</dd>
		<dt class="col-sm-2">
			Адрес
		</dt>
		<dd class="col-sm-10">
			@Model.Address
		</dd>
		<dt class="col-sm-2">
			Тип
		</dt>
		<dd class="col-sm-10">
			@Model.RealtyObjectKind
		</dd>
		<dt class="col-sm-2">
			Описание
		</dt>
		<dd class="col-sm-10">
			@Model.Description
		</dd>
	</dl>
</div>

<form asp-action="DeleteConfirmed">
	<input type="hidden" asp-for="Id" />
	<input type="submit" value="Удалить" class="btn btn-danger" /> |
	<a asp-action="Index">Назад</a>
</form>