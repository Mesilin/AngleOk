@model Data.AngleOk.Model.Models.RealtyObject

@{
	ViewData["Title"] = "Delete Realty Object";
}

<h2>Delete Realty Object</h2>

<h3>Are you sure you want to delete this?</h3>
<div>
	<h4>RealtyObject</h4>
	<hr />
	<dl class="row">
		<dt class="col-sm-2">
			Кадастровый номер
		</dt>
		<dd class="col-sm-10">
			@Model.CadastralNumber
		</dd>
		<dt class="col-sm-2">
			Адрес
		</dt>
		<dd class="col-sm-10">
			@Model.Address
		</dd>
		<dt class="col-sm-2">
			Тип
		</dt>
		<dd class="col-sm-10">
			@Model.RealtyObjectKind
		</dd>
		<dt class="col-sm-2">
			Описание
		</dt>
		<dd class="col-sm-10">
			@Model.Description
		</dd>
	</dl>
</div>

<form asp-action="DeleteConfirmed">
	<input type="hidden" asp-for="Id" />
	<input type="submit" value="Delete" class="btn btn-danger" /> |
	<a asp-action="Index">Back to List</a>
</form>