@model Data.AngleOk.Model.Models.RealtyObject

@{
	ViewData["Title"] = "Create Realty Object";
}

<h2>Create Realty Object</h2>

<form asp-action="Create2">
	<div class="form-group">
		<label asp-for="CadastralNumber" class="control-label"></label>
		<input asp-for="CadastralNumber" class="form-control" />
		<span asp-validation-for="CadastralNumber" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="Address" class="control-label"></label>
		<input asp-for="Address" class="form-control" />
		<span asp-validation-for="Address" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="RealtyObjectKind" class="control-label"></label>
		<select asp-for="RealtyObjectKind" class="form-control">
			@foreach (var kind in Enum.GetValues(typeof(Data.AngleOk.Model.Enums.RealtyObjectKind)))
			{
				<option value="@kind">@kind</option>
			}
		</select>
		<span asp-validation-for="RealtyObjectKind" class="text-danger"></span>
	</div>
	<div class="form-group">
		<label asp-for="Description" class="control-label"></label>
		<input asp-for="Description" class="form-control" />
		<span asp-validation-for="Description" class="text-danger"></span>
	</div>
	<div class="form-group">
		<input type="submit" value="Create" class="btn btn-primary" />
	</div>
</form>

<div>
	<a asp-action="Index">Back to List</a>
</div>